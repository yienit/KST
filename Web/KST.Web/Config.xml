<?xml version="1.0" encoding="utf-8" ?>
<!-- 
	说明：该文件为拨号器云端配置文件，非系统维护人员请勿修改该文件内容。
-->
<Config>

  <Basic>

    <!-- 数据库连接字符串 -->
    <DBConnection>Server=localhost;Port=3306;Database=kst;Uid=root;Pwd=abc123__</DBConnection>

    <!-- 日志文件保存天数 -->
    <MaxLogFileCount>30</MaxLogFileCount>

  </Basic>

  <Security>

    <!-- 同个IP地址获取短信验证码的间隔时间(单位：秒) -->
    <CaptchaGetTimeSpan>120</CaptchaGetTimeSpan>

    <!-- 短信验证码过期时间(单位：秒) -->
    <CaptchaExpireTime>300</CaptchaExpireTime>
  
</Security>

  <Sms>

    <!-- 云片网络短信平台Api Key -->
    <SmsApiKey>d1ee367311068544a523f30b4bb38b14</SmsApiKey>

    <Template>

      <!-- 注册账号时短信验证码模板 -->
      <RegTemplate>【考试通】您正在注册考试通账号，验证码为{0}，请输入验证码完成注册。</RegTemplate>

      <!-- 找回密码时短信验证码模板 -->
      <GetPasswordTemplate>【考试通】您正在找回密码，验证码为{0}，请输入验证码找回密码。</GetPasswordTemplate>

    </Template>

  </Sms>

  <AppVersion>

    <!-- PC客户端最新版本、下载链接及更新日志 -->
    <PC>
      <Version></Version>
      <DownloadUrl></DownloadUrl>
      <ChangeLog></ChangeLog>
    </PC>

    <!-- Android客户端最新版本、下载链接及更新日志 -->
    <Android>
      <Version></Version>
      <DownloadUrl></DownloadUrl>
      <ChangeLog></ChangeLog>
    </Android>

    <!-- IOS客户端最新版本、下载链接及更新日志 -->
    <IOS>
      <Version></Version>
      <DownloadUrl></DownloadUrl>
      <ChangeLog></ChangeLog>
    </IOS>

  </AppVersion>

</Config>